{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "ServiceRequest",
        "id":"{{ID}}",
        {{#if ORC-1}},
            "intent":"order",
        {{/if}},
        {{#if (eq OBR-11 "G")}},
            "intent":"reflex-order",
        {{/if}},
        {{#if (ne OBR-11 "G")}},
            "intent":"order",
        {{/if}},
        "identifier":
        [
            {
                {{#if (not OBR-2)}},
                    {{>DataType/EIIdentifier.hbs EI=ORC-2}},
                {{/if}},
                "type":
                {
                    "coding":
                    [
                        {
                            {{#if (not OBR-2)}},
                                "code":"PLAC",
                            {{/if}},
                            {{#if OBR-2}},
                                "code":"PLAC",
                            {{/if}},
                            {{#if OBR-3}},
                                "code":"FILL",
                            {{/if}},
                            {{#if OBR-53}},
                                "code":"PLAC",
                            {{/if}},
                            {{#if (not OBR-2)}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                            {{#if OBR-2}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                            {{#if OBR-3}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                            {{#if OBR-53}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                        },
                    ],
                },
            },
            {
                {{>DataType/EIIdentifier.hbs EI=ORC-3}},
                "type":
                {
                    "coding":
                    [
                        {
                            {{#if ORC-3}},
                                "code":"FILL",
                            {{/if}},
                            {{#if ORC-3}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                        },
                    ],
                },
            },
            {
                {{>DataType/EIIdentifier.hbs EI=ORC-4}},
                "type":
                {
                    "coding":
                    [
                        {
                            {{#if ORC-4}},
                                "code":"PGN",
                            {{/if}},
                            {{#if ORC-4}},
                                "system":"http://terminology.hl7.org/CodeSystem/v2-0203",
                            {{/if}},
                        },
                    ],
                },
            },
            {{#if OBR-2}},
                { {{>DataType/EIIdentifier.hbs EI=OBR-2}} },
            {{/if}},
            { {{>DataType/EIIdentifier.hbs EI=OBR-3}} },
            { {{>DataType/EIIdentifier.hbs EI=OBR-53}} },
        ],
        "status":{{>DataType/_code.hbs mapping="CodeSystem/OrderStatus.hbs" inCode=ORC-5 }},
        {{#if ORC-1}},
            "authoredOn":"{{formatAsDateTime ORC-9}}",
        {{/if}},
        "occurrenceDateTime":"{{formatAsDateTime ORC-15}}",
        "occurrenceDateTime":"{{formatAsDateTime OBR-6}}",
        "locationCode":
        [
            { {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/OrderType.hbs" CWE=ORC-29}} },
        ],
        "quantityQuantity":
        {
            {{>DataType/CQ.hbs CQ=TQ1-2}},
        },
        "occurrenceTiming":
        {
            "repeat":
            {
                "boundsDuration":
                {
                    {{>DataType/CQ.hbs CQ=TQ1-6}},
                },
                "boundsPeriod":
                {
                    "start":"{{formatAsDateTime TQ1-7}}",
                    "end":"{{formatAsDateTime TQ1-8}}",
                },
                {{>DataType/CQServiceRequestduration.hbs CQ=TQ1-13}},
                "count":"{{TQ1-14}}",
            },
        },
        "priority":"{{TQ1-9}}",
        "priority":{{>DataType/_code.hbs mapping="CodeSystem/Priority.hbs" inCode=OBR-5 }},
        "code":
        {
            {{>DataType/CWECodeableConcept.hbs CWE=OBR-4}},
        },
        "reasonCode":
        [
            { {{>DataType/CWECodeableConcept.hbs CWE=OBR-31}} },
        ],
