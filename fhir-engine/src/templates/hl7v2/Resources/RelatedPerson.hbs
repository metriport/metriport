{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "RelatedPerson",
        "id":"{{ID}}",
        "identifier":
        [
            { {{>DataType/CX.hbs CX=PID-21}} },
            { {{>DataType/EIIdentifier.hbs EI=ROL-1}} },
            {
                {{>DataType/PLN.hbs PLN=PRT-24}},
            },
        ],
        "relationship":
        [
            { {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/Role.hbs" CWE=ROL-3}} },
            { {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/Relationship.hbs" CWE=NK1-3}} },
            {
                {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/Relationship.hbs" CWE=NK1-7}},
            },
        ],
        {{>DataType/XCNRelatedPerson.hbs XCN=ROL-4}},
        "period":
        {
            "start":"{{formatAsDateTime ROL-5}}",
            "start":"{{formatAsDateTime NK1-8}}",
            "start":"{{formatAsDateTime PRT-11}}",
            "end":"{{formatAsDateTime ROL-6}}",
            "end":"{{formatAsDateTime NK1-9}}",
            "end":"{{formatAsDateTime PRT-12}}",
        },
        "address":
        [
            { {{>DataType/XAD.hbs XAD=ROL-11}} },
            {
                {{>DataType/XAD.hbs XAD=NK1-4}},
            },
            {
                {{>DataType/XAD.hbs XAD=NK1-32}},
            },
            { {{>DataType/XAD.hbs XAD=PRT-14}} },
        ],
        "telecom":
        [
            { {{>DataType/XTN.hbs XTN=ROL-12}} },
            {{#each (getFieldRepeats NK1-5)}}
                {
                    {{>DataType/XTN.hbs XTN=this}},
                },
            {{/each}}
            {
                {{>DataType/XTN.hbs XTN=NK1-6}},
                {{#if NK1-6}},
                    "use":"work",
                {{/if}},
            },
            {
                {{>DataType/XTN.hbs XTN=NK1-31}},
            },
            {
                {{>DataType/XTN.hbs XTN=NK1-40}},
            },
            {
                {{>DataType/XTN.hbs XTN=NK1-41}},
            },
            { {{>DataType/XTN.hbs XTN=PRT-15}} },
        ],
        "name":
        [
            { {{>DataType/XPN.hbs XPN=NK1-2}} },
            { {{>DataType/XPN.hbs XPN=NK1-30}} },
        ],
        "gender":{{>DataType/_string.hbs mapping="CodeSystem/Gender.hbs" inCode=NK1-15 }},
        "birthDate":"{{addHyphensDate NK1-16}}",
        "communication":
        [
            {
                "language":
                {
                    {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/Language.hbs" CWE=NK1-20}},
                },
            },
        ],
        {{>DataType/XCNRelatedPerson.hbs XCN=PRT-5}},
    },
    "request":{
        "method":"PUT",
        "url":"RelatedPerson/{{ID}}",
    },
},
