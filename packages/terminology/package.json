{
  "name": "terminology",
  "version": "1.2.0",
  "description": "embedded term server",
  "main": "app.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "ts-node src/app.ts",
    "dev": "PORT=8666 ts-node-dev --transpile-only --respawn --inspect=0.0.0.0:9230 --project tsconfig.dev.json --files src/app.ts",
    "clean": "rimraf dist && rimraf node_modules",
    "build": "tsc -p tsconfig.build.json",
    "prettier-fix": "npx prettier '**/*.{js,ts,md}' --write --ignore-path ../../.prettierignore",
    "seed-lookup": "npx --node-options='--max-old-space-size=8192' ts-node src/seed/seedUmlsLookup.ts",
    "seed-crosswalk": "npx --node-options='--max-old-space-size=8192' ts-node src/seed/seedUmlsCrosswalk.ts",
    "seed-snomed-icd10": "npx --node-options='--max-old-space-size=8192' ts-node src/seed/seedSnomedIcd10Map.ts",
    "seed-ndc-lookup": "npx --node-options='--max-old-space-size=8192' ts-node src/seed/seed-ndc-from-nlm.ts",
    "seed-fda-descriptions": "npx --node-options='--max-old-space-size=8192' ts-node src/seed/seed-ndc-from-fda.ts",
    "load-test": "artillery run src/__tests__/load-test.yml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metriport/metriport.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/metriport/metriport/issues"
  },
  "homepage": "https://github.com/metriport/metriport#readme",
  "dependencies": {
    "@medplum/core": "^3.2.5",
    "@medplum/fhir-router": "^3.2.5",
    "@medplum/fhirtypes": "^3.2.5",
    "@metriport/shared": "^0.23.3",
    "aws-sdk": "^2.1243.0",
    "axios": "^1.8.2",
    "csv-parser": "^3.2.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "form-data": ">=4.0.4",
    "http-status": "^2.0.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "unzip-stream": "^0.3.4",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.800.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.x",
    "@types/unzip-stream": "^0.3.4",
    "@types/uuid": "^10.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
