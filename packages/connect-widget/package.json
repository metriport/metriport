{
  "name": "connect-widget",
  "version": "1.4.19",
  "private": true,
  "scripts": {
    "clean": "rimraf build && rimraf node_modules",
    "start": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` craco start",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` craco build",
    "build:cloud": "if [ \"$ENV_TYPE\" = \"staging\" ]; then REACT_APP_GIT_SHA=`git rev-parse --short HEAD` env-cmd -f .env.staging craco build; else npm run build; fi",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint src/**/*.ts src/**/*.tsx",
    "lint-fix": "npm run lint --fix",
    "prettier-fix": "npx prettier '**/*.{ts,tsx}' --write",
    "test": "jest --runInBand --detectOpenHandles --passWithNoTests",
    "test:e2e": "REACT_APP_ENV_TYPE=development npx playwright test '.*\\.test\\.\\e2e\\.ts'",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.12",
    "@chakra-ui/react": "^2.4.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@metriport/api-sdk": "^7.3.3",
    "@sentry/react": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "axios": "^0.27.2",
    "env-cmd": "^10.1.0",
    "framer-motion": "^7.6.15",
    "jest": "^29.6.2",
    "posthog-js": "^1.51.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "ts-jest": "^29.1.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@playwright/test": "^1.37.0",
    "@tsconfig/create-react-app": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  }
}
