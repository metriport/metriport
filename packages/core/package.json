{
  "name": "@metriport/core",
  "version": "1.13.7",
  "private": true,
  "description": "Metriport helps you access and manage health and medical data, through a single open source API. Common code shared across packages.",
  "author": "Metriport Inc. <contact@metriport.com>",
  "homepage": "https://metriport.com/",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./domain/*": "./dist/domain/*.js",
    "./command/*": "./dist/command/*.js",
    "./external/*": "./dist/external/*.js",
    "./util": "./dist/util/index.js",
    "./util/*": "./dist/util/*.js",
    "./mpi/*": "./dist/mpi/*.js",
    "./fhir-to-cda/*": "./dist/fhir-to-cda/*.js",
    "./shareback/*": "./dist/shareback/*.js"
  },
  "typesVersions": {
    "*": {
      "/": [
        "dist/index.d.ts"
      ],
      "domain": [
        "dist/domain/index.d.ts"
      ],
      "domain/*": [
        "dist/domain/*"
      ],
      "command": [
        "dist/command/index.d.ts"
      ],
      "command/*": [
        "dist/command/*"
      ],
      "external": [
        "dist/external/index.d.ts"
      ],
      "external/*": [
        "dist/external/*"
      ],
      "util": [
        "dist/util/index.d.ts"
      ],
      "util/*": [
        "dist/util/*"
      ],
      "mpi": [
        "dist/mpi/index.d.ts"
      ],
      "mpi/*": [
        "dist/mpi/*"
      ],
      "fhir-to-cda": [
        "dist/fhir-to-cda/index.d.ts"
      ],
      "fhir-to-cda/*": [
        "dist/fhir-to-cda/*"
      ],
      "shareback": [
        "dist/shareback/index.d.ts"
      ],
      "shareback/*": [
        "dist/shareback/*"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf dist && rimraf node_modules",
    "tsc": "tsc",
    "watch": "tsc --watch",
    "build": "tsc -p .",
    "build:cloud": "npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint . --ext .ts",
    "lint-fix": "npm run lint --fix",
    "prettier-fix": "npx prettier '**/*.ts' --write",
    "test": "jest --runInBand --detectOpenHandles --passWithNoTests",
    "test:e2e": "E2E=true jest --runInBand --detectOpenHandles --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.435.0",
    "@aws-sdk/s3-request-presigner": "^3.435.0",
    "@faker-js/faker": "^8.0.2",
    "@metriport/api-sdk": "file:packages/api-sdk",
    "@metriport/commonwell-sdk": "file:packages/commonwell-sdk",
    "@metriport/ihe-gateway-sdk": "file:packages/ihe-gateway-sdk",
    "@opensearch-project/opensearch": "^2.3.1",
    "asn1js": "^3.0.5",
    "aws-sdk": "^2.1243.0",
    "dayjs": "^1.11.9",
    "formidable": "^3.5.1",
    "http-status": "^1.7.0",
    "jaro-winkler": "^0.2.8",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "pkijs": "^3.0.16",
    "playwright": "^1.39.0",
    "pvutils": "^1.1.3",
    "sequelize": "^6.37.1",
    "whatwg-mimetype": "^4.0.0",
    "xml-crypto": "^6.0.0",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@medplum/fhirtypes": "^2.0.32",
    "@tsconfig/recommended": "^1.0.2",
    "@types/formidable": "^3.4.5",
    "@types/jest": "29.5.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/pg": "^8.11.6",
    "@types/whatwg-mimetype": "^3.0.2",
    "@types/xml2js": "^0.4.14",
    "@types/xmldom": "^0.1.32",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.3",
    "ts-essentials": "^9.3.1",
    "ts-jest": "29.1.1",
    "typescript": "^4.9.5"
  }
}