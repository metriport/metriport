{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Practitioner",
        "id":"{{ID}}",
        "meta": 		{
		    "profile": [
			    "http://hl7.org/fhir/us/core/StructureDefinition/us-core-practitioner"
			]
		},
        "identifier":
  		[
        	{{#each (toArray practitioner.id)}}
            	{{>DataType/Identifier.hbs id=this}},
            {{/each}}
        ],
        "name":
        [
        	{{#each (toArray practitioner.assignedPerson.name)}}
        		{{>DataType/HumanName.hbs name=this}},
            {{/each}}
        ],
        "address":
        [
        	{{#each (toArray practitioner.addr)}}
        		{{>DataType/Address.hbs addr=this}},
            {{/each}}
        ],
        "telecom":
        [
        	{{#each (toArray practitioner.telecom)}}
        		{{>DataType/ContactPoint.hbs contactPoint=this}},
            {{/each}}
        ],
        "qualification":
        [
            {{#each (toArray practitioner.code)}}            
        		{
                "code": {{>DataType/CodeableConcept.hbs code=this}}
                },
            {{/each}}
        ],
    },
    "request":{
        "method":"PUT",
        "url":"Practitioner/{{ID}}",
    },
},
