    "test": "jest --passWithNoTests",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/jest": "^29.2.0",
    "@types/node": "18.11.7",
    "@types/prettier": "2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "aws-cdk": "2.79.1",
    "esbuild": "^0.15.12",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.2.2",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.79.1",
    "constructs": "^10.1.144",
    "source-map-support": "^0.5.21"
  }
