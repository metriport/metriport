{
  "name": "infrastructure",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "build:cloud": "npm run build",
    "watch": "tsc -w",
    "typecheck": "tsc --noEmit",
